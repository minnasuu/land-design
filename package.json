{
  "name": "@suminhan/land-design",
  "private": false,
  "version": "0.0.100",
  "author": {
    "name": "suminhan"
  },
  "main": "./lib/index.umd.js",
  "style": "./lib/style.css",
  "module": "./lib/index.es.js",
  "types": "./lib/land-design/packages/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.umd.js",
      "import": "./lib/index.es.js",
      "types": "./lib/land-design/packages/index.d.ts"
    },
    "./style.css": "./lib/style.css",
    "./lib/style.css": "./lib/style.css",
    "./styles": "./lib/style.css",
    "./css": "./lib/style.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "lib/style.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:styles": "node scripts/build-styles.js",
    "build:lib": "npm run build:styles && tsc && vite build --config vite.config.lib.ts",
    "lib": "bash updateVersion.sh --auto && npm run build:lib && pnpm publish --no-git-checks",
    "publish": "bash scripts/publish.sh",
    "deploy": "gh-pages -d dist",
    "test": "vitest",
    "new": "node scripts/create-component.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-modules-umd": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-typescript": "8.5.0",
    "@testing-library/react": "^14.2.1",
    "@types/node": "^18.11.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "clsx": "^1.2.1",
    "gh-pages": "^6.1.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.2",
    "vite": "^5.4.10",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@figma/code-connect": "^1.3.4",
    "@tailwindcss/vite": "^4.1.11",
    "@vitejs/plugin-react-swc": "^3.7.1",
    "axios": "^1.7.7",
    "jsdom": "^20.0.3",
    "motion": "^12.23.0",
    "path": "^0.12.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.1.1",
    "react-syntax-highlighter": "^15.6.1",
    "rollup": ">=2.14.0 <3.0.0",
    "sass": "^1.72.0",
    "styled-components": "^6.1.8",
    "tslib": "^2.6.3",
    "vite-plugin-dts": "^4.3.0"
  }
}
