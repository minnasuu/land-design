#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/bin/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/bin/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/@figma+code-connect@1.3.4/node_modules:/Users/uus/Documents/projects/minnasuuGitHub/land-design/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/bin/figma" "$@"
else
  exec node  "$basedir/../.pnpm/@figma+code-connect@1.3.4/node_modules/@figma/code-connect/bin/figma" "$@"
fi
